<h1>Edit Review</h1>



<body class="display">
  <%= form_tag "/reviews/#{@review.id}", method: :patch do %>
  <%= label_tag :title %>
  <%= text_field_tag :title %>
  <%= label_tag :rating %>
  <%= text_field_tag :rating %>
  <%= label_tag :content %>
  <%= text_field_tag :content %>
  <%= label_tag :picture %>
  <%= text_field_tag :picture  %>

  <%= submit_tag 'Edit Review' %>
  <% end %>
</body>
